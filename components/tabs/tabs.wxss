
.container{
    display: flex;
    flex-direction: column;
   min-height: 100%;
   
}
.tabs{
    display: flex;
    padding: 30rpx 0;
    background-color: #fff;
    align-items: center;
    position: sticky;
    top:0;
    z-index: 99;

    /* height: 126rpx;
    box-sizing: border-box; */
    
}
.tab-item{
    flex:1;
    display: flex;
    flex-direction: column;
    align-items: center;
}
.tab-label{
    color: #888888;
    white-space: nowrap;
}
.active-tab{
    color: #333333;
    white-space: bold;
}
.divider{
    height: 6rpx;
    width: 50rpx;
    margin-top: 10rpx;
    background-color: #f3d066;
}
.tab-panel{
    flex:1;
    margin-top: 30rpx;
}